{
  "name": "swap-selection-cursor",
  "displayName": "Swap Selection Cursor",
  "description": "swap selection area cursor end-to-end position",
  "publisher": "hellokidder",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "git@github.com:hellokidder/swap-selection-cursor.git"
  },
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swap-selection-cursor.SwapSelectionCursor",
        "title": "Swap Cursor Position in Selection"
      }
    ],
    "keybindings": [
      {
        "command": "swap-selection-cursor.SwapSelectionCursor",
        "key": "ctrl+shift+o",
        "when": "editorTextFocus && editorHasSelection"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}